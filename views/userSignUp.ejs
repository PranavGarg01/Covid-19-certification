<!-- Web3js library link -->
<script src="https://cdn.jsdelivr.net/npm/web3@latest/dist/web3.min.js"></script>


<script src="bundle.js"></script><!--To use Eth-Crypto library to decrypt data-->
<script src="userSignUp.js"></script><!--the js script for the page-->

<!-- Bootstrap links below -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>



<div class="container">
<h2>User Sign Up</h2>
<form class="form-horizontal" action="#">
  <div class="form-group">
    <label class="control-label col-sm-2" >Email:</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="username" placeholder="Enter username" name="username">
    </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2" for="pwd">Password:</label>
    <div class="col-sm-10">          
      <input type="password" class="form-control" id="pwd" placeholder="Enter password" name="pwd">
    </div>
  </div>
  <div class="form-group">        
    <div class="col-sm-offset-2 col-sm-10">
      <div class="checkbox">
        <label>Private Key<h4 id="prvKey"></h4></label>
        <label>Public Key<h4 id="pubKey"></h4></label>
        <label>Address<h4 id="addr"></h4></label>
      </div>
    </div>
  </div>
  <div class="form-group">        
    <div class="col-sm-offset-2 col-sm-10">
      <!-- below is the submit button-->
      <button type="button" onclick="createAccount()" class="btn btn-default">Submit</button>
    </div>
  </div>
</form>
</div>

  